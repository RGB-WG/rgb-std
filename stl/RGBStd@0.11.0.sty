@mnemonic(vanilla-static-scoop)
data AnchoredBundle    : tapret#1 ClientBundleTapretProof
                       | opret ClientBundleOpretProof

@mnemonic(legend-amazon-crystal)
data ClientBundleOpretProof : mpcProof CommitVerify.MerkleProof
                       , dbcProof BPCore.OpretProof
                       , bundle RGBCommit.TransitionBundle

@mnemonic(liberal-morgan-absorb)
data ClientBundleTapretProof : mpcProof CommitVerify.MerkleProof
                       , dbcProof BPCore.TapretProof
                       , bundle RGBCommit.TransitionBundle

@mnemonic(gravity-bucket-dance)
data Consignmentfalse  : version ContainerVer
                       , transfer Std.Bool
                       , terminals {RGBCommit.BundleId -> BPCore.SecretSeal}
                       , genesis RGBCommit.Genesis
                       , bundles {WitnessBundle ^ ..0xffffffff}
                       , schema RGBCommit.Schema
                       , types StrictTypes.TypeSystem
                       , scripts {AluVM.Lib ^ ..0x400}
                       , attachments {RGBCommit.AttachId -> [Byte ^ ..0xffffff]}
                       , signatures {ContentId -> ^ ..0xff ContentSigs}

@mnemonic(trapeze-montana-bravo)
data Consignmenttrue   : version ContainerVer
                       , transfer Std.Bool
                       , terminals {RGBCommit.BundleId -> BPCore.SecretSeal}
                       , genesis RGBCommit.Genesis
                       , bundles {WitnessBundle ^ ..0xffffffff}
                       , schema RGBCommit.Schema
                       , types StrictTypes.TypeSystem
                       , scripts {AluVM.Lib ^ ..0x400}
                       , attachments {RGBCommit.AttachId -> [Byte ^ ..0xffffff]}
                       , signatures {ContentId -> ^ ..0xff ContentSigs}

@mnemonic(giant-bravo-jacket)
data ContainerVer      : v2#2 | (|)


@mnemonic(lunar-eagle-beyond)
data ContentId         : schema RGBCommit.SchemaId
                       | genesis RGBCommit.ContractId

@mnemonic(apropos-horizon-couple)
data ContentSigs       : {RGBCommit.Identity -> ^ 1..0xa SigBlob}

@mnemonic(transit-alfred-nixon)
data Kit               : version ContainerVer
                       , schemata {RGBCommit.Schema ^ ..0xff}
                       , types StrictTypes.TypeSystem
                       , scripts {AluVM.Lib}
                       , signatures {ContentId -> ^ ..0xff ContentSigs}

@mnemonic(paper-visa-storm)
data PubWitness        : txid Bitcoin.Txid
                       | tx Bitcoin.Tx

@mnemonic(insect-cello-avalon)
data SigBlob           : [Byte ^ 1..0x1000]

@mnemonic(message-danube-casino)
data WitnessBundle     : pubWitness PubWitness, anchoredBundle AnchoredBundle


